{
  "applicationId": "DesktopSingux",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/public/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": false
  },
  "nativeAllowList": [
"app.*",
"os.*",
"clipboard.*",
"computer.*",
"debug.*",
"events.*",
"filesystem.*",
"clipboard.*",
"window.*"
  ],
  "modes": {
    "window": {
      "title": "DesktopSingux",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "center": true,
      "fullScreen": true,
      "alwaysOnTop": false,
      "icon": "/public/assets/img/favicon.png",
      "enableInspector": true,
      "borderless": false,
      "transparent": true,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": false,
      "useSavedState": false
    }
  },
  "cli": {
    "binaryName": "DesktopSingux",
    "resourcesPath": "/public/",
    "clientLibrary": "/public/assets/scripts/neutralino.js",
    "binaryVersion": "5.4.0",
    "clientVersion": "5.4.0"
  },
  "chrome": {
    "width": 800,
    "height": 600,
    "args":  "--user-agent=\"Neutralinojs chrome mode\"",
    "nativeBlockList": [
      "filesystem.*",
      "os.*"
    ]
  }
}